@charset "UTF-8";

main {
	padding: 50px;
	margin: auto;
}

a {
	color: blue;
	text-decoration: none;
}

ul a {
	color: black;
}

h3 {
	text-align: center;
	height: 40px;
}

.noti_top {
	text-align: center;
}

.noti {
	font-size: 14pt;
}

.noti2 {
	font-size: 10pt;
	color: gray;
	height: 50px;
}

main ul {
	text-align: center;
	width: 907px;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 0px 0px;
}

main li {
	display: inline-block;
	width: 300px;
	height: 270px;
	font-size: 14px;
	vertical-align: top;
	padding: 0px 0px 0px 0px;
	margin: 0px 0px 0px 0px;
}

