table {
	width: 100%;
}

th {
	padding: 10px;
	border-bottom: solid 1px gray;
	text-align: left;
	font-size: 11pt;
	color: darkcyan;
}

td {
	padding: 10px;
	border-bottom: solid 1px lightgray;
	text-align: left;
}

.notib::before {
	content: "* ";
	color: blue;
}

caption {
	padding: 10px;
	border-bottom: solid 1px black;
	text-align: left;
	font-size: 11pt;
	font-weight: bold;
}

.marginb {
	margin-bottom: 150px;
}

.scheck {
