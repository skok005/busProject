.e_pass {
	position: relative;
	height: 215px;
	background: url("../info/img/info_card.png") 45% 0 no-repeat;
	background-size: contain;
}

.e_pass p {
	position: absolute;
	left: 45%;
	margin-left: 100px;
	font-size: 14px;
}

.e_pass .card_tag {
	top: 85px;
}

.e_pass span {
	display: block;
	font-size: 13px;
}

