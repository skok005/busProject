@charset "UTF-8";
*{
	font-size: 12pt;
}
a{
	color: rgb(52,85,139);
	text-decoration: none;
	cursor: pointer;
}
a:hover{
	color: rgb(52,180,180);
}
.table{
	font-size: 12pt;
}
table{
	width: 800px;
	border-bottom: 3px solid #777;
}
.noticeTitle{
	text-align: center;
	border-bottom: 3px solid #777;
	padding: 20px;
	padding-bottom: 80px
}
h1{
	font-size: 40pt;
	color: rgb(52,85,139);
	margin-top: 50px;
}
p{
	color: #aaa;
	font-size: 10pt;
	margin-top: -15px;
}
th{
	font-size: 14pt;
	text-align: left;
	vertical-align: middle;
	background: rgba(52,85,139,0.2);
	border-bottom: 1px solid #ccc;
	padding: 5px;
}
font{
	color: #777;
	font-size: 10pt;
	font-weight: 200;
	float: right;
}
td{
	padding-top: 5px;
	padding-bottom: 5px;
}
.content{
	height: 400px;
	border-bottom: 1px solid #ccc;
	vertical-align: top;
	padding: 10px;
}
.content div{
	white-space: pre-line;
}